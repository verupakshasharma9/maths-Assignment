import java.util.*;
class Rev
{
    public static void main(String []a)
    {
        Scanner s=new Scanner(System.in);
        System.out.println("Enter a numer");
        int n=s.nextInt();
        int rev=0;
        int rem=0;
        while(n!=0)
        {
            rem=n%10;
            rev=rev*10+rem;
            n=n/10;
        }
        System.out.println(rev);
    }
}
